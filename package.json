{
  "name": "odgn-bitfield",
  "version": "1.0.0",
  "description": "a javascript bitfield implementation",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "typings": "dist/index.d.ts",
  "umd:main": "dist/index.umd.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "start": "microbundle watch",
    "build": "microbundle",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odogono/bitfield.git"
  },
  "keywords": [
    "bitfield"
  ],
  "author": "alexander veenendaal <alex@opendoorgonorth.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/odogono/bitfield/issues"
  },
  "homepage": "https://github.com/odogono/bitfield#readme",
  "devDependencies": {
    "microbundle": "^0.4.4",
    "@types/jest": "^22.1.0",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1"
  },

  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "js"]
  }
}
