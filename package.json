  "license": "MIT",
  "bugs": {
    "url": "https://github.com/odogono/bitfield/issues"
  },
  "homepage": "https://github.com/odogono/bitfield#readme",
  "devDependencies": {
    "microbundle": "^0.6.0",
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(js|ts)$",
